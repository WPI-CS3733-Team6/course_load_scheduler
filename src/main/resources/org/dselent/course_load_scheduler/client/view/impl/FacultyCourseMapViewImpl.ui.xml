<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.panel-grey {
		background: #DCDCDC;
		}
	</ui:style>
	<g:HTMLPanel ui:field="rootPanel">
		<g:LayoutPanel width="100%" height="100%" styleName="{style.panel-grey}">
			<g:layer>
				<g:ScrollPanel width="100%" height="100%" alwaysShowScrollBars="true">
			<g:LayoutPanel width="100%" height="280px" ui:field="layoutPanel" styleName="{style.panel-grey}">
			<g:layer left="0.0pct" top="1.5pct" right="0.0pct" bottom="88.6pct">
				<g:Label text="Faculty Course Map" horizontalAlignment="ALIGN_CENTER" ui:field="titleLabel" width="100%" height="100%" styleName="h1"/>
			</g:layer>
			<g:layer top="11.8pct" bottom="0.0pct" right="0.0pct" left="0.0pct">
				<g:FlexTable ui:field="dataTable" width="100%" height="88.9%"/>
			</g:layer>
		</g:LayoutPanel>
		</g:ScrollPanel>
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 