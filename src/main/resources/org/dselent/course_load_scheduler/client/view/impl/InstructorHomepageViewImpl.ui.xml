<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.button-blue {
		color: white;
		font-size: 12px;
		font-family: Times
		New Roman;
		background: #0558b2;
		}

		.button-bluelight {
		color: white;
		font-size: 12px;
		font-family: Times New Roman;
		background: #05abc1;
		}

		.panel-grey {
		background: #DCDCDC;
		}

		.panel-greydark {
		background: #989b9b;
		}

	</ui:style>
	
	<g:HTMLPanel width="430" height="300" ui:field="rootPanel">
		<g:LayoutPanel width="100%" height="100%" addStyleNames="{style.panel-grey}" ui:field="layoutPanel">
			<g:layer left="2.0pct" top="3.0pct" right="2.0pct" bottom="80.0pct">
				<g:HorizontalPanel width="100%" height="100%"
					horizontalAlignment="ALIGN_CENTER" ui:field="selectPanel">
					<g:Button ui:field="currentClassButton" text="Current Classes" width="100%"
						height="100%%" />
					<g:Button text="Search for Classes" width="100%" height="100%%" ui:field="classSearchButton" />
					<g:Button width="100%" height="100%%" ui:field="accountInfoButton">Account Info</g:Button>
					<g:Button width="100%" height="100%%" ui:field="courseCartButton">Course Registration Cart</g:Button>
				</g:HorizontalPanel>
			</g:layer>
			<g:layer left="25.0pct" top="20.0pct" bottom="72.0pct" right="25.0pct">
				<g:Button text="Faculty Course Map"
					width="100%" addStyleNames="{style.button-blue}" height="100%" ui:field="facultyCourseMapButton" />
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 