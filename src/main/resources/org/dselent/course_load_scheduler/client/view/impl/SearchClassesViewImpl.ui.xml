<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

		.button-blue {
		color: white;
		font-size: 12px;
		font-family: Helvetica;
		background: DodgerBlue;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:LayoutPanel width="100%" height="20%">
			<g:layer left="40px" width="189px" top="51px" height="28px">
				<g:ListBox ui:field="Semester" name="semester"/>
			</g:layer>
			<g:layer left="711px" width="83px" top="51px" height="28px">
				<g:Button>Search</g:Button>
			</g:layer>
			<g:layer left="40px" width="189px" top="25px" height="16px">
				<g:Label text="Semester" horizontalAlignment="ALIGN_CENTER"/>
			</g:layer>
			<g:layer left="235px" width="189px" top="25px" height="16px">
				<g:Label text="Subject" horizontalAlignment="ALIGN_CENTER"/>
			</g:layer>
			<g:layer left="235px" width="189px" top="51px" height="28px">
				<g:ListBox name="semester" ui:field="Subject"/>
			</g:layer>
			<g:layer left="506px" width="138px" top="51px" height="28px">
				<g:Button ui:field="button" addStyleNames="{style.button-blue}">Advanced Options</g:Button>
			</g:layer>
		</g:LayoutPanel>
		<g:LayoutPanel width="100%" height="80%">
			<g:layer left="87px" width="132px" top="73px" height="359px">
				<g:ListBox visibleItemCount="5" height="100%" ui:field="Course_Name"/>
			</g:layer>
			<g:layer left="87px" width="132px" top="51px" height="16px">
				<g:Label text="Course Name" horizontalAlignment="ALIGN_CENTER"/>
			</g:layer>
			<g:layer left="225px" width="132px" top="73px" height="359px">
				<g:ListBox height="100%" visibleItemCount="5" ui:field="Registration_Status"/>
			</g:layer>
			<g:layer left="225px" width="132px" top="51px" height="16px">
				<g:Label horizontalAlignment="ALIGN_CENTER" text="Registration Status"/>
			</g:layer>
			<g:layer left="363px" width="132px" top="73px" height="359px">
				<g:ListBox height="100%" visibleItemCount="5" ui:field="Course_Number"/>
			</g:layer>
			<g:layer left="363px" width="132px" top="51px" height="16px">
				<g:Label horizontalAlignment="ALIGN_CENTER" text="Course Number"/>
			</g:layer>
			<g:layer left="501px" width="132px" top="73px" height="359px">
				<g:ListBox height="100%" visibleItemCount="5"/>
			</g:layer>
			<g:layer left="501px" width="132px" top="51px" height="16px">
				<g:Label horizontalAlignment="ALIGN_CENTER" text="Schedule (Days)"/>
			</g:layer>
			<g:layer left="639px" width="132px" top="73px" height="359px">
				<g:ListBox height="100%" visibleItemCount="5"/>
			</g:layer>
			<g:layer left="639px" width="132px" top="51px" height="16px">
				<g:Label horizontalAlignment="ALIGN_CENTER" text="Expected Population"/>
			</g:layer>
		</g:LayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 